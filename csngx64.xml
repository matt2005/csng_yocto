<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default remote="jumpnow" revision="default"/>

  <!-- You can have as many remote repositories as you like -->

  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="qt5" fetch="git://github.com/meta-qt5" />
  <remote name="jumpnow" fetch="git://github.com/jumpnow" />
  <remote name="opencardev" fetch="git://github.com/opencardev" />
  <remote name="schnitzeltony" fetch="git://github.com/schnitzeltony" />

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="poky" revision="refs/heads/hardknott" remote="yocto" path="poky-hardknott"/>
  <project name="meta-raspberrypi" revision="refs/heads/hardknott"  remote="yocto" path="poky-hardknott/meta-raspberrypi" />
  <project name="meta-security" revision="refs/heads/hardknott"  remote="yocto" path="poky-hardknott/meta-security" />
  <project name="meta-openembedded" revision="refs/heads/hardknott"  remote="oe" path="poky-hardknott/meta-openembedded" />
  <project name="meta-qt5" revision="refs/heads/hardknott"  remote="qt5" path="poky-hardknott/meta-qt5" />
  <project name="meta-virtualization" revision="refs/heads/hardknott" remote="yocto" path="poky-hardknott/meta-virtualization" />
  <project name="meta-opencardev" revision="refs/heads/hardknott"  remote="opencardev" path="poky-hardknott/meta-opencardev" />
  <project name="meta-qt5-extra" revision="refs/heads/hardknott"  remote="schnitzeltony" path="poky-hardknott/meta-qt5-extra" />
  <project name="meta-jumpnow" revision="refs/heads/hardknott"  remote="jumpnow" path="poky-hardknott/meta-jumpnow" />
  <project name="meta-rpi64" revision="refs/heads/hardknott"  remote="jumpnow" path="poky-hardknott/meta-rpi64" />

</manifest>
